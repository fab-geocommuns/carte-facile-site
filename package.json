{
  "name": "carte-facile",
  "version": "1.1.0",
  "description": "Un service pour intégrer facilement des cartes dans des applications et sites internet.",
  "scripts": {
    "build": "npx @11ty/eleventy --pathprefix 'carte-facile-site'",
    "postbuild": "npx pagefind --site _site/ --output-subdir \"_pagefind\"",
    "start": "npx @11ty/eleventy --serve --quiet",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
    "benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:codegouvfr/eleventy-dsfr.git"
  },
  "author": {
    "name": "Startups d'Etat Fonds de cartes, à la Fabrique des géocommuns (IGN)",
    "email": "fonds-de-cartes@beta.gouv.fr",
    "url": "https://beta.gouv.fr/startups/?incubateur=fab-geocommuns"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "homepage": "https://fab-geocommuns.github.io/carte-facile-site",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^1.0.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@codegouvfr/eleventy-plugin-calendar": "^3.0.5",
    "@codegouvfr/eleventy-plugin-i18n": "^0.1.3",
    "@gouvfr/dsfr": "^1.13.0",
    "ics": "^3.2.0",
    "luxon": "^3.5.0",
    "markdown-it-anchor": "^8.6.6",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-container": "^4.0.0",
    "pagefind": "^1.3.0"
  },
  "dependencies": {
    "carte-facile": "^0.5.16",
    "maplibre-gl": "^5.2.0"
  }
}
